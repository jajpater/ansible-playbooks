# rofi-rbw configuration file
# Generated by Ansible - modifications may be overwritten

action = {{ rofi_rbw_config.action }}
target = {{ rofi_rbw_config.target }}
prompt = {{ rofi_rbw_config.prompt }}
{% if rofi_rbw_config.rofi_args %}
rofi-args = {{ rofi_rbw_config.rofi_args }}
{% endif %}
clear-after = {{ rofi_rbw_config.clear_after }}
{% if rofi_rbw_config.no_symbols %}
no-symbols
{% endif %}

# Keybindings
{% for key, binding in rofi_rbw_config.keybindings.items() %}
{{ key.replace('_', '-') }} = {{ binding }}
{% endfor %}
